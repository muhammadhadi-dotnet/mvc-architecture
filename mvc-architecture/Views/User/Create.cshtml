@model User;

@{
}
    <h3 class="mb-4">Add New User</h3>
    <form action="Create" method="post">
        <div class="mb-3">
            <label asp-for="Name" class="form-label"> Name</label>
            <input type="text" asp-for="Name" class="form-control" placeholder="Enter your name">
        </div>
        <div class="mb-3">
        <label asp-for="Email" class="form-label"> Email</label>
        <input type="text" asp-for="Email" class="form-control" placeholder="Enter your email">
        </div>
        <div class="mb-3">
        <label asp-for="Age" class="form-label"> Age</label>
        <input type="text" asp-for="Age" class="form-control" placeholder="Enter Your Age">
        </div>
    <div class="mb-3">
        <label asp-for="Address" class="form-label"> Address</label>
        <input type="text" asp-for="Address" class="form-control" placeholder="Enter Adress">
    </div>
        <button type="submit" class="btn btn-primary">Add User</button>
    </form>
